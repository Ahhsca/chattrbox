]').each((idx, element) => {\n        let $element = $(element);\n        let timestamp = new Date().setTime($element.attr('data-time'));\n        let ago = moment(timestamp).fromNow();\n        $element.html(ago);\n      });\n    }, 1000);\n  }\n}"},"lineCount":null}},"error":null,"hash":"cb2de59f41f711c2d975421cd5fa1fa5","cacheData":{"env":{}}}